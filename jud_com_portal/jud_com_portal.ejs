<div class="table-responsive" id="listen">
    <br><br><br><br><br>
    <div class="block-heading">
        <center><h2 class="text-info">Competition Portal</h2>
        
        </center>
    </div>
    <% if(jud_com_data.length>0){ %>
    <table class="table css-serial">
        <thead>
            <th scope="col">Sl.no</th>
            <th scope="col">Competent</th>
            <th scope="col">Click to play the song.</th>
        </thead>
        <tbody>
            <% for(var i=0;i < jud_com_data.length;i++){ %>
                <tr>
                <td></td>
                <td><%= jud_com_data[i].full_name %></td>
                <td>
                    <form action="/play_com_song" method="POST">
                        <input type="hidden" name="com_path" value="<%= jud_com_data[i].com_song_path %>">
                        <input type="hidden" name="com_name" value="<%= jud_com_data[i].full_name %>">
                        <button type="submit" class="btn btn-primary" value="Upload Audio" name="submit">Click to play song</button>
                    </form>
                </td>
            </tr>
            <% } %>
        </tbody>
    </table>
</div>

<hr>
<div class="block-heading">
    <center><h2 class="text-info">Select the winner</h2>
    <br>
    <form action="/select_winner" method="post">
        <input type="hidden" name="com_id" value="<%= part_list[0].com_id %>">
        <select name="winner" required>
            <option disabled selected value> -- select an option -- </option>
            <% for(var i=0;i < jud_com_data.length;i++){ %>
                <option name="winner" id="winner"><%= jud_com_data[i].full_name %></option>
            <% } %>
        </select>
        <button type="submit" class="btn btn-primary" onclick="myFunction()">Submit winner</button>
    </form>
</div>
<% }else{ %>
    <center><h3>No Audios yet.</h3></center>
<% } %>
<script>
function myFunction() {
    alert("Winner has been submitted!!");
}
</script>
